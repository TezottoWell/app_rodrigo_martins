{
  "expo": {
    "name": "team-Rodrigo-Martins",
    "slug": "team-rodrigo-martins",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.well-dev.team-rodrigo-martins",
      "buildNumber": "2",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "NSFaceIDUsageDescription": "Este aplicativo precisa usar o Face ID para autenticação segura",
        "NSMotionUsageDescription": "Este app precisa acessar os sensores de movimento para contar seus passos",
        "NSHealthShareUsageDescription": "Este app precisa acessar seus dados de saúde para contar seus passos",
        "NSHealthUpdateUsageDescription": "Este app precisa permissão para salvar seus dados de passos no HealthKit",
        "LSApplicationQueriesSchemes": [
          "youtube",
          "https",
          "http"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.welldev.teamrodrigomartins",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACTIVITY_RECOGNITION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Este aplicativo precisa usar o Face ID para autenticação segura"
        }
      ],
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos para alterar a imagem de perfil."
        }
      ],
      [
        "expo-sensors",
        {
          "motionPermission": "Allow $(PRODUCT_NAME) to access your device motion"
        }
      ]
    ],
    "androidStatusBar": {
      "backgroundColor": "#1a1a1a",
      "barStyle": "light-content",
      "translucent": false
    },
    "extra": {
      "eas": {
        "projectId": "00a29f85-11f2-432c-a291-9834002a36d4"
      }
    },
    "owner": "lui_zwellington"
  }
}
